<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_season" model="ir.sequence">
            <field name="name">Season Ref</field>
            <field name="code">season.ref</field>
            <field name="prefix">SE</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_dist_plan" model="ir.sequence">
            <field name="name">Distribution Plan Ref</field>
            <field name="code">dist.plan.ref</field>
            <field name="prefix">DP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_prod_plan" model="ir.sequence">
            <field name="name">Production Plan Ref</field>
            <field name="code">prod.plan.ref</field>
            <field name="prefix">PP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>